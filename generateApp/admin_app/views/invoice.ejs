<body style="background-color: #fff;">
<% if (typeof(order) !== 'undefined' && order != null && order.length > 0) { let formating=Intl.NumberFormat('en-US')  %>
<div id="top" class="sa-app__body card_a4">
    <div class="sa-invoice">
      <div class="sa-invoice__card">
        <div class="sa-invoice__header">
          <div class="sa-invoice__meta">
            <div class="sa-invoice__title">Bon de sortie</div>
            <div class="sa-invoice__meta-items">
              <strong>Date : </strong> <%= order[0].order_date.toISOString().substr(0,10) %>
              <br>
              <strong>Référence : </strong> <%= order[0].order_ref %>
            </div>
          </div>
          <div class="sa-invoice__logo">
            <!-- invoice-logo -->
            <div class="sa-invoice-logo">
              <div class="sa-invoice-logo__body">
                <div class="sa-invoice-logo__letters">
                  <img src="/assets/images/logo1.png" width="150"   alt="" />
                </div>
              </div>
            </div>
            <!-- invoice-logo / end -->
          </div>
        </div>
        <div class="sa-invoice__sides">
          <div class="sa-invoice__side">
            <div class="sa-invoice__side-title">Bon pour</div>
            <div class="sa-invoice__side-name"><%= order[0].user_name +" "+ order[0].user_lastname %></div>
            <div class="sa-invoice__side-meta">
              <%= order[0].user_address +" - "+ order[0].wilaya_name %><br /><%= order[0].user_phone %>
            </div>
          </div>
          <div class="sa-invoice__side">
            <div class="sa-invoice__side-title">Fournisseur</div>
            <div class="sa-invoice__side-name">Literie d'amour</div>
            <div class="sa-invoice__side-meta">
              Rue 20 aout commune bordj bahri<br /> 0561 44 64 14 <br />
              contact@literiedamour.dz
            </div>
          </div>
        </div>
        <div class="sa-invoice__table-container">
          <table class="sa-invoice__table">
            <thead class="sa-invoice__table-head">
              <tr>
                <th class="sa-invoice__table-column--type--product">
                Produit
                </th>
                <th class="sa-invoice__table-column--type--price">
                  Prix
                </th>
                <th class="sa-invoice__table-column--type--unit">
                 
                </th>
                <th class="sa-invoice__table-column--type--quantity">
                  Qantité
                </th>
                <th class="sa-invoice__table-column--type--total">
                  Total
                </th>
              </tr>
            </thead>
            <tbody class="sa-invoice__table-body">
              <% if (typeof(order_p) !== 'undefined' && order_p != null && order_p.length > 0) { %>
              <% for (var i = 0; i < order_p.length; i++) { %>
              <tr>
                <td class="sa-invoice__table-column--type--product">
                  <%= order_p[i].product_name %>
                </td>
                <td class="sa-invoice__table-column--type--price">
                  <%= formating.format(order_p[i].product_price_o)+" DA" %>
                </td>
                <td class="sa-invoice__table-column--type--unit">
                 
                </td>
                <td class="sa-invoice__table-column--type--quantity">
                  <%= order_p[i].product_qt_o %>
                </td>
                <td class="sa-invoice__table-column--type--total">
                  <%= formating.format(order_p[i].product_qt_o * order_p[i].product_price_o)+" DA" %>
                </td>
              </tr>
              <% }} %>
            </tbody>
            <tbody class="sa-invoice__table-totals">
              <tr>
                <th
                  class="sa-invoice__table-column--type--header"
                  colspan="4"
                >
                  Livraison
                </th>
                <td class="sa-invoice__table-column--type--total">
                  <%= formating.format(order[0].delivery_price)+" DA" %> 
                </td>
              </tr>
              <!-- <tr>
                <th
                  class="sa-invoice__table-column--type--header"
                  colspan="4"
                >
                  Tax (VAT 20%)
                </th>
                <td class="sa-invoice__table-column--type--total">
                  $108.00
                </td>
              </tr> -->
              <!-- <tr>
                <th
                  class="sa-invoice__table-column--type--header"
                  colspan="4"
                >
                  Discount
                </th>
                <td class="sa-invoice__table-column--type--total">
                  -$50.00
                </td>
              </tr> -->
            </tbody>
          </table>
        </div>
        <div class="sa-invoice__total">
          <div class="sa-invoice__total-title">Total :</div>
          <div class="sa-invoice__total-value"><%= formating.format(order[0].order_total_price + order[0].delivery_price)+" DA" %></div>
        </div>
        
      </div>
      <div class=" footer_a4 d-flex justify-content-between">
        <h4>Signature Fournisseur</h4>
        <h4>Signature Client</h4>
      </div>
    </div>
  </div>
  <% } else { %>

    <div class="alert alert-danger m-5" role="alert">
     
      Aucune commande correspondante !
 
    </div>

  <% } %>



